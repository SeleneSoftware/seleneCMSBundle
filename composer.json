{
    "name": "selene/cms-bundle",
    "type": "library",
    "version": "0.0.0",
    "description": "CMS platform for Symfony",
    "homepage": "https://github.com/SeleneSoftware/seleneCMSBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Marshall",
            "homepage": "https://github.com/psion"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "twig/extra-bundle": "^3.4",
        "twig/markdown-extra": "^3.4",
        "twig/twig": "^2.12|^3.0",
        "symfony/runtime": "6.2.*",
        "symfony/security-bundle": "6.2.*",
        "symfony/security-csrf": "6.2.*",
        "symfony/framework-bundle": "6.2.*"
    },
    "autoload": {
        "psr-4": {
            "selene\\CMSBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "selene\\CMSBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
